
CC	:= gcc
CXXFLAGS := -g
    
OBJECTS	:= dep/read.o dep/write.o #main.o
INCLUDES = -I includes
all: read.o libfile_partition.a

#my_program: $(OBJECTS)
#	$(CXX) $(OBJECTS) -o my_program

read.o: src/read.c $(INCLUDE)
	$(CC) $(INCLUDES) $(CXXFLAGS) -c src/read.c -o dep/read.o
libfile_partition.a:
	ar rcs lib/libfile_partition.a dep/*.o
#main.o: main.cpp
#	$(CC) $(INCLUDES) $(CXXFLAGS) -c main.cpp -o main.o
clean:
	rm dep/*.o
	rm lib/*.a

